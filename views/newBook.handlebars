<label for="title">Book Title</label>
<input type="text" id="title" name="title">

<button onclick="searchGBooks()" id="submitButton">Submit New Book</button>
<div class="bookresults">
    <ul>
        {{#each book}}
        <li>{{book}}</li>
        <li>{{title}}</li>
        {{/each}}
    </ul>
</div>

<script>

     function searchGBooks() {
        titleInp = document.querySelector("#title")
        const title = titleInp.value

        fetch(`/api/books/GBooks/${title}`).then(function(result){
            
            result.json()
            .then(function(data){
                console.log("I want this to return something: ", data.items[0].volumeInfo.title);
            })
        }
        )
        //console.log("searchGBooks -> response", response)
    }
</script>